{
  "services": {
    "name": "promotion",
    "version": "v1",
    "style": "RPC"
  },
  "models": {
    "promotionModel": "./models/promotionModel"
  },
  "promotions": {
    "classes": {
      "default": "./classes/default"
    }
  },
  "bus": {
    "engines": {
      "local": {
        "handler": "./local"
      },
      "amqp": {
        "handler": "./amqp",
        "url": "amqp://bus"
      }
    },
    "channels": {
      "promotions": {
        "name": "PROMOTIONS",
        "engine": "amqp"
      }
    }
  }
}
